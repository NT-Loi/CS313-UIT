{
    "arxiv_id": "2012.11142",
    "title": "Towards Incorporating Entity-specific Knowledge Graph Information in Predicting Drug-Drug Interactions",
    "authors": [
        {
            "name": "I Mondal",
            "citations_all": 1628,
