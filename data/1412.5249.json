{
    "arxiv_id": "1412.5249",
    "title": "The switch Markov chain for sampling irregular graphs",
    "authors": [
        {
            "name": "C Greenhill",
            "citations_all": 3312,
            "citations_recent": 1303,
            "h_index_all": 33,
            "h_index_recent": 20,
            "i10_index_all": 57,
            "i10_index_recent": 37
