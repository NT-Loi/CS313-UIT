{
    "arxiv_id": "1712.06349",
    "title": "Understanding Quantum Algorithms via Query Complexity",
    "authors": [
        {
            "name": "A Ambainis",
            "citations_all": 15340,
            "citations_recent": 6214,
            "h_index_all": 57,
