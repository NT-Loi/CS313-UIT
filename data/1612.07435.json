{
    "arxiv_id": "1612.07435",
    "title": "Partial $\\ell_1$ optimization in random linear systems -- phase transitions and large deviations",
    "authors": [
        {
            "name": "M Stojnic",
            "citations_all": null,
            "citations_recent": null,
