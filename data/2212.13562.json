{
    "arxiv_id": "2212.13562",
    "title": "An effectivization of the law of large numbers for algorithmically random sequences and its absolute speed limit of convergence",
    "authors": [
        {
            "name": "K Tadaki",
            "citations_all": null,
            "citations_recent": null,
            "h_index_all": null,
