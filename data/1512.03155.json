{
    "arxiv_id": "1512.03155",
    "title": "Enhanced image feature coverage: Key-point selection using genetic algorithms",
    "authors": [
        {
            "name": "E Bostanci",
            "citations_all": 2041,
            "citations_recent": 1539,
            "h_index_all": 25,
