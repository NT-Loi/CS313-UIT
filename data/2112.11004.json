{
    "arxiv_id": "2112.11004",
    "title": "Point spread function estimation for blind image deblurring problems based on framelet transform",
    "authors": [
        {
            "name": "R Parvaz",
            "citations_all": null,
            "citations_recent": null,
            "h_index_all": null,
