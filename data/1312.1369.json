{
    "arxiv_id": "1312.1369",
    "title": "Quasi-Polynomial Time Approximation Scheme for Sparse Subsets of Polygons",
    "authors": [
        {
            "name": "S Har-Peled",
            "citations_all": 13832,
            "citations_recent": 4500,
