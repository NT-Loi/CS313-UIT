{
    "arxiv_id": "1412.4031",
    "title": "High-level numerical simulations of noise in CCD and CMOS photosensors: review and tutorial",
    "authors": [
        {
            "name": "M Konnik",
            "citations_all": 332,
            "citations_recent": 191,
