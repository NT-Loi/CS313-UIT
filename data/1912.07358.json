{
    "arxiv_id": "1912.07358",
    "title": "Blind Denoising Autoencoder",
    "authors": [
        {
            "name": "A Majumdar",
            "citations_all": 7976,
            "citations_recent": 4625,
            "h_index_all": 48,
            "h_index_recent": 33,
            "i10_index_all": 165,
            "i10_index_recent": 113
