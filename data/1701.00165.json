{
    "arxiv_id": "1701.00165",
    "title": "Improved Stereo Matching with Constant Highway Networks and Reflective Confidence Learning",
    "authors": [
        {
            "name": "A Shaked",
            "citations_all": null,
            "citations_recent": null,
