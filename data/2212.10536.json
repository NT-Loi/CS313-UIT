{
    "arxiv_id": "2212.10536",
    "title": "Measure More, Question More: Experimental Studies on Transformer-based Language Models and Complement Coercion",
    "authors": [
        {
            "name": "Y Gu",
            "citations_all": 2085,
            "citations_recent": 2083,
