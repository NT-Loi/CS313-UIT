{
    "arxiv_id": "2101.10451",
    "title": "Evolution of Small Cell from 4G to 6G: Past, Present, and Future",
    "authors": [
        {
            "name": "V Sathya",
            "citations_all": 1699,
            "citations_recent": 1409,
