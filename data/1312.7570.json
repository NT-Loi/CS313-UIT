{
    "arxiv_id": "1312.7570",
    "title": "Actions in the Eye: Dynamic Gaze Datasets and Learnt Saliency Models for Visual Recognition",
    "authors": [
        {
            "name": "S Mathe",
            "citations_all": 515,
