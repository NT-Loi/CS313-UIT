{
    "arxiv_id": "1612.08254",
    "title": "Fredkin and Toffoli gates implemented in Oregonator model of Belousov-Zhabotinsky medium",
    "authors": [
        {
            "name": "A Adamatzky",
            "citations_all": 18824,
