{
    "arxiv_id": "1612.01842",
    "title": "An Improved One-to-All Broadcasting in Higher Dimensional Eisenstein-Jacobi Networks",
    "authors": [
        {
            "name": "Z Hussain",
            "citations_all": 64,
            "citations_recent": 47,
            "h_index_all": 4,
