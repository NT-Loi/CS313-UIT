    "arxiv_id": "1212.6527",
    "title": "Discovering Basic Emotion Sets via Semantic Clustering on a Twitter Corpus",
    "authors": [
        {
            "name": "EY Bann",
            "citations_all": 76,
            "citations_recent": 26,
            "h_index_all": 3,
