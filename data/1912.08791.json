{
    "arxiv_id": "1912.08791",
    "title": "Forecasting significant stock price changes using neural networks",
    "authors": [
        {
            "name": "F Kamalov",
            "citations_all": 5671,
            "citations_recent": 5587,
            "h_index_all": 33,
