{
    "arxiv_id": "1612.03785",
    "title": "A model and sensitivity analysis of the quality economics of defect-detection techniques",
    "authors": [
        {
            "name": "S Wagner",
            "citations_all": 10690,
            "citations_recent": 6032,
            "h_index_all": 50,
            "h_index_recent": 38,
