{
    "arxiv_id": "1512.01210",
    "title": "Nearly optimal separations between communication (or query) complexity and partitions",
    "authors": [
        {
            "name": "R Kothari",
            "citations_all": 5144,
            "citations_recent": 4091,
            "h_index_all": 26,
            "h_index_recent": 24,
            "i10_index_all": 43,
            "i10_index_recent": 38
