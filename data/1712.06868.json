{
    "arxiv_id": "1712.06868",
    "title": "Heinrich Behmann's Contributions to Second-Order Quantifier Elimination from the View of Computational Logic",
    "authors": [
        {
            "name": "C Wernhard",
            "citations_all": null,
            "citations_recent": null,
            "h_index_all": null,
            "h_index_recent": null,
