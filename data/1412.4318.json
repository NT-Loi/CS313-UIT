{
    "arxiv_id": "1412.4318",
    "title": "Adaptive Resource Management for Multimedia Applications in Femtocellular and Macrocellular Networks",
    "authors": [
        {
            "name": "MZ Chowdhury",
            "citations_all": null,
            "citations_recent": null,
            "h_index_all": null,
