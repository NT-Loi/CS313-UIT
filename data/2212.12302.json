{
    "arxiv_id": "2212.12302",
    "title": "Novel Node-Based BAT for Finding All Minimal Cuts",
    "authors": [
        {
            "name": "WC Yeh",
            "citations_all": 10969,
            "citations_recent": 5643,
            "h_index_all": 59,
