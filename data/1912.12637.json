{
    "arxiv_id": "1912.12637",
    "title": "DEFT-FUNNEL: an open-source global optimization solver for constrained grey-box and black-box problems",
    "authors": [
        {
            "name": "PR Sampaio",
            "citations_all": 219,
            "citations_recent": 147,
            "h_index_all": 8,
            "h_index_recent": 7,
