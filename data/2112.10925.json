{
    "arxiv_id": "2112.10925",
    "title": "DB-BERT: a Database Tuning Tool that \"Reads the Manual\"",
    "authors": [
        {
            "name": "I Trummer",
            "citations_all": 1870,
            "citations_recent": 1521,
            "h_index_all": 23,
            "h_index_recent": 19,
            "i10_index_all": 43,
            "i10_index_recent": 38
