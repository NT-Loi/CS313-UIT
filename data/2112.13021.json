{
    "arxiv_id": "2112.13021",
    "title": "Noninvasive Fetal Electrocardiography: Models, Technologies and Algorithms",
    "authors": [
        {
            "name": "R Sameni",
            "citations_all": 6241,
            "citations_recent": 3583,
