{
    "arxiv_id": "1612.06835",
    "title": "Box constrained $\\ell_1$ optimization in random linear systems -- asymptotics",
    "authors": [
        {
            "name": "M Stojnic",
            "citations_all": null,
            "citations_recent": null,
            "h_index_all": null,
