{
    "arxiv_id": "2012.10925",
    "title": "A distributed hypergraph model for the full-scale simulation of the collaborations in dblp",
    "authors": [
        {
            "name": "Z Xie",
            "citations_all": null,
            "citations_recent": null,
            "h_index_all": null,
