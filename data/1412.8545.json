{
    "arxiv_id": "1412.8545",
    "title": "Semantics for a Quantum Programming Language by Operator Algebras",
    "authors": [
        {
            "name": "K Cho",
            "citations_all": 505,
            "citations_recent": 356,
            "h_index_all": 11,
            "h_index_recent": 8,
            "i10_index_all": 11,
            "i10_index_recent": 5
