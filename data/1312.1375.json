{
    "arxiv_id": "1312.1375",
    "title": "Impact of receiver reaction mechanisms on the performance of molecular communication networks",
    "authors": [
        {
            "name": "CT Chou",
            "citations_all": 6671,
            "citations_recent": 1414,
