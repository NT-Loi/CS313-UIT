{
    "arxiv_id": "1312.2355",
    "title": "On the dependency on the size of the data when chasing under conceptual dependencies",
    "authors": [
        {
            "name": "D Martinenghi",
            "citations_all": 2413,
            "citations_recent": 798,
            "h_index_all": 32,
