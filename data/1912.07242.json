{
    "arxiv_id": "1912.07242",
    "title": "More Data Can Hurt for Linear Regression: Sample-wise Double Descent",
    "authors": [
        {
            "name": "P Nakkiran",
            "citations_all": 3932,
            "citations_recent": 3652,
            "h_index_all": 26,
            "h_index_recent": 26,
            "i10_index_all": 41,
            "i10_index_recent": 39
