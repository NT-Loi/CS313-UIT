{
    "arxiv_id": "2301.11275",
    "title": "A Review of Scene Representations for Robot Manipulators",
    "authors": [
        {
            "name": "C Sifferman",
            "citations_all": 69,
            "citations_recent": 69,
            "h_index_all": 5,
