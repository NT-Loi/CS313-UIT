{
    "arxiv_id": "1901.04977",
    "title": "An open-source sensor platform for analysis of group dynamics",
    "authors": [
        {
            "name": "M Hopfengaertner",
            "citations_all": null,
            "citations_recent": null,
            "h_index_all": null,
