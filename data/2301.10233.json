{
    "arxiv_id": "2301.10233",
    "title": "Introducing Political Ecology of Creative-Ai",
    "authors": [
        {
            "name": "A Holzapfel",
            "citations_all": 2383,
            "citations_recent": 1235,
            "h_index_all": 25,
            "h_index_recent": 17,
