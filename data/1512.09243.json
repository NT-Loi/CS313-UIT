{
    "arxiv_id": "1512.09243",
    "title": "Computational Complexity of Some Quantum Theories in $1+1$ Dimensions",
    "authors": [
        {
            "name": "S Mehraban",
            "citations_all": 463,
            "citations_recent": 424,
