{
    "arxiv_id": "2101.10816",
    "title": "Application Layer Modeling in Vehicle Networks: Cooperative Maneuver Use Case",
    "authors": [
        {
            "name": "S Platt",
            "citations_all": 16,
            "citations_recent": 15,
            "h_index_all": 2,
