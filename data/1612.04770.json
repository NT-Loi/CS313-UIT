{
    "arxiv_id": "1612.04770",
    "title": "Detect, Replace, Refine: Deep Structured Prediction For Pixel Wise Labeling",
    "authors": [
        {
            "name": "S Gidaris",
            "citations_all": 10953,
            "citations_recent": 9907,
