{
    "arxiv_id": "1512.04751",
    "title": "Design and Analysis of Secure Exam Protocols",
    "authors": [
        {
            "name": "R Giustolisi",
            "citations_all": 389,
            "citations_recent": 215,
            "h_index_all": 12,
