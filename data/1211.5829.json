{
    "arxiv_id": "1211.5829",
    "title": "An Automatic Algorithm for Object Recognition and Detection Based on ASIFT Keypoints",
    "authors": [
        {
            "name": "R Oji",
            "citations_all": null,
            "citations_recent": null,
