{
    "arxiv_id": "2012.13716",
    "title": "Hybrid and Non-Uniform quantization methods using retro synthesis data for efficient inference",
    "authors": [
        {
            "name": "R Kumar",
            "citations_all": 22,
            "citations_recent": 22,
