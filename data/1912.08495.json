{
    "arxiv_id": "1912.08495",
    "title": "Provable Non-Convex Optimization and Algorithm Validation via Submodularity",
    "authors": [
        {
            "name": "YA Bian",
            "citations_all": 5243,
            "citations_recent": 5017,
            "h_index_all": 33,
            "h_index_recent": 32,
            "i10_index_all": 59,
            "i10_index_recent": 56
