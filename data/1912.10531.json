    "arxiv_id": "1912.10531",
    "title": "The CoCo-Beholder: Enabling Comprehensive Evaluation of Congestion Control Algorithms",
    "authors": [
        {
            "name": "E Khasina",
            "citations_all": null,
            "citations_recent": null,
            "h_index_all": null,
