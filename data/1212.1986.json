{
    "arxiv_id": "1212.1986",
    "title": "WorkingWiki: a MediaWiki-based platform for collaborative research",
    "authors": [
        {
            "name": "L Worden",
            "citations_all": 1490,
            "citations_recent": 1187,
            "h_index_all": 20,
