{
    "arxiv_id": "1501.00105",
    "title": "Face recognition using color local binary pattern from mutually independent color channels",
    "authors": [
        {
            "name": "G Anbarjafari",
            "citations_all": 8845,
            "citations_recent": 5440,
            "h_index_all": 45,
            "h_index_recent": 38,
            "i10_index_all": 120,
            "i10_index_recent": 96
