{
    "arxiv_id": "1612.07130",
    "title": "Sparse Coding of Neural Word Embeddings for Multilingual Sequence Labeling",
    "authors": [
        {
            "name": "G Berend",
            "citations_all": 758,
            "citations_recent": 410,
