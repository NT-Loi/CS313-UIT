{
    "arxiv_id": "2412.20871",
    "title": "Simplified integrity checking for an expressive class of denial constraints",
    "authors": [
        {
            "name": "D Martinenghi",
            "citations_all": 2411,
            "citations_recent": 796,
            "h_index_all": 32,
