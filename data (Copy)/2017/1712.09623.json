    "arxiv_id": "1712.09623",
    "title": "An empirical evaluation for the intrusion detection features based on machine learning and feature selection methods",
    "authors": [
        {
            "name": "M Alkasassbeh",
            "citations_all": 3428,
            "citations_recent": 2878,
            "h_index_all": 34,
