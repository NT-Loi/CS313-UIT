{
    "arxiv_id": "1712.02529",
    "title": "Enabling the Remote Acquisition of Digital Forensic Evidence through Secure Data Transmission and Verification",
    "authors": [
        {
            "name": "M Scanlon",
            "citations_all": 2813,
            "citations_recent": 2238,
            "h_index_all": 27,
