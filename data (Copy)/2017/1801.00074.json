{
    "arxiv_id": "1801.00074",
    "title": "Randomized Communication in Radio Networks",
    "authors": [
        {
            "name": "BS Chlebus",
            "citations_all": 2868,
            "citations_recent": 625,
            "h_index_all": 30,
