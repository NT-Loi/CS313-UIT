    "arxiv_id": "1712.06768",
    "title": "User modeling for point-of-interest recommendations in location-based social networks: the state-of-the-art",
    "authors": [
        {
            "name": "S Liu",
            "citations_all": null,
            "citations_recent": null,
            "h_index_all": null,
