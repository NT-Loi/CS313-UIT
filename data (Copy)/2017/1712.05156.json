{
    "arxiv_id": "1712.05156",
    "title": "Analysis of LTE-A Heterogeneous Networks with SIR-based Cell Association and Stochastic Geometry",
    "authors": [
        {
            "name": "G Giambene",
            "citations_all": 4747,
            "citations_recent": 1770,
