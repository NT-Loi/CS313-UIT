{
    "arxiv_id": "1712.03455",
    "title": "Study of Peer-to-Peer Network Based Cybercrime Investigation: Application on Botnet Technologies",
    "authors": [
        {
            "name": "M Scanlon",
            "citations_all": 2821,
            "citations_recent": 2245,
            "h_index_all": 27,
