{
    "arxiv_id": "1712.09687",
    "title": "Combining Representation Learning with Logic for Language Processing",
    "authors": [
        {
            "name": "T Rockt√§schel",
            "citations_all": 29310,
            "citations_recent": 26932,
            "h_index_all": 58,
