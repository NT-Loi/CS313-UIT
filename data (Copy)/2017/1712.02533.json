{
    "arxiv_id": "1712.02533",
    "title": "Parallel Prefix Algorithms for the Registration of Arbitrarily Long Electron Micrograph Series",
    "authors": [
        {
            "name": "M Copik",
            "citations_all": 838,
            "citations_recent": 810,
            "h_index_all": 15,
