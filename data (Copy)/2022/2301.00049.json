{
    "arxiv_id": "2301.00049",
    "title": "Multi-Finger Haptics: Analysis of Human Hand Grasp towards a Tripod Three-Finger Haptic Grasp model",
    "authors": [
        {
            "name": "J James",
            "citations_all": 175,
            "citations_recent": 82,
            "h_index_all": 8,
