    "arxiv_id": "2212.11136",
    "title": "It is not \"accuracy vs. explainability\" -- we need both for trustworthy AI systems",
    "authors": [
        {
            "name": "D Petkovic",
            "citations_all": null,
            "citations_recent": null,
            "h_index_all": null,
