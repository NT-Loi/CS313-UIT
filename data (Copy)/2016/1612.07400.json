{
    "arxiv_id": "1612.07400",
    "title": "The \"paradox\" of computability and a recursive relative version of the Busy Beaver function",
    "authors": [
        {
            "name": "FS Abrahao",
            "citations_all": 305,
            "citations_recent": 237,
            "h_index_all": 11,
