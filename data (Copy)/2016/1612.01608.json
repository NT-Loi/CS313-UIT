{
    "arxiv_id": "1612.01608",
    "title": "AI Researchers, Video Games Are Your Friends!",
    "authors": [
        {
            "name": "J Togelius",
            "citations_all": 27148,
            "citations_recent": 15175,
            "h_index_all": 83,
