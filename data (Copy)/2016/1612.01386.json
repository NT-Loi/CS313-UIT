{
    "arxiv_id": "1612.01386",
    "title": "Universality of the SIS prevalence in networks",
    "authors": [
        {
            "name": "P Van Mieghem",
            "citations_all": 23965,
            "citations_recent": 10024,
            "h_index_all": 74,
