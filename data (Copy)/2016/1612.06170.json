{
    "arxiv_id": "1612.06170",
    "title": "Crowd collectiveness measure via graph-based node clique learning",
    "authors": [
        {
            "name": "W Ren",
            "citations_all": null,
            "citations_recent": null,
            "h_index_all": null,
