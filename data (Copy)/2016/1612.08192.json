{
    "arxiv_id": "1612.08192",
    "title": "An Improved Homomorphism Preservation Theorem From Lower Bounds in Circuit Complexity",
    "authors": [
        {
            "name": "B Rossman",
            "citations_all": null,
            "citations_recent": null,
            "h_index_all": null,
