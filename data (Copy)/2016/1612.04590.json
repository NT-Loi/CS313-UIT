{
    "arxiv_id": "1612.04590",
    "title": "A literature survey of the quality economics of defect-detection techniques",
    "authors": [
        {
            "name": "S Wagner",
            "citations_all": 10690,
            "citations_recent": 6032,
            "h_index_all": 50,
            "h_index_recent": 38,
            "i10_index_all": 171,
            "i10_index_recent": 123
