{
    "arxiv_id": "1912.12443",
    "title": "Construction of mutually unbiased maximally entangled bases in $\\mathbb{C}^{2^s}\\otimes\\mathbb{C}^{2^s}$ by using Galois rings",
    "authors": [
        {
            "name": "D Xu",
            "citations_all": null,
