{
    "arxiv_id": "1912.09797",
    "title": "Axiomatizing rectangular grids with no extra non-unary relations",
    "authors": [
        {
            "name": "E Kopczy≈Ñski",
            "error": "invalid literal for int() with base 10: ''"
