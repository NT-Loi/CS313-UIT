{
    "arxiv_id": "1912.06933",
    "title": "Global and country-specific mainstreaminess measures: Definitions, analysis, and usage for improving personalized music recommendation systems",
    "authors": [
        {
            "name": "C Bauer",
            "citations_all": 3284,
