{
    "arxiv_id": "1312.0697",
    "title": "Levels of discontinuity, limit-computability, and jump operators",
    "authors": [
        {
            "name": "M de Brecht",
            "citations_all": null,
            "citations_recent": null,
            "h_index_all": null,
