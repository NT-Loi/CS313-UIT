{
    "arxiv_id": "1312.1325",
    "title": "Permutation polynomials induced from permutations of subfields, and some complete sets of mutually orthogonal latin squares",
    "authors": [
        {
            "name": "ME Zieve",
            "citations_all": 1606,
            "citations_recent": 655,
