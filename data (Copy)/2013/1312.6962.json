{
    "arxiv_id": "1312.6962",
    "title": "Subjectivity Classification using Machine Learning Techniques for Mining Feature-Opinion Pairs from Web Opinion Sources",
    "authors": [
        {
            "name": "A Kamal",
            "citations_all": 206,
            "citations_recent": 113,
