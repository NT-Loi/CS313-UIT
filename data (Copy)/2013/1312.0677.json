    "arxiv_id": "1312.0677",
    "title": "Formal Model of Web Service Composition: An Actor-Based Approach to Unifying Orchestration and Choreography",
    "authors": [
        {
            "name": "Y Wang",
            "citations_all": null,
            "citations_recent": null,
            "h_index_all": null,
