{
    "arxiv_id": "1311.5768",
    "title": "An RKHS Approach to Estimation with Sparsity Constraints",
    "authors": [
        {
            "name": "A Jung",
            "citations_all": 1871,
            "citations_recent": 1425,
            "h_index_all": 23,
            "h_index_recent": 20,
