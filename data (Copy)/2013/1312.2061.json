{
    "arxiv_id": "1312.2061",
    "title": "Region and Location Based Indexing and Retrieval of MR-T2 Brain Tumor Images",
    "authors": [
        {
            "name": "BG Prasad",
            "citations_all": 648,
            "citations_recent": 252,
