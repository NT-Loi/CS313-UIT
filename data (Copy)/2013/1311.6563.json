{
    "arxiv_id": "1311.6563",
    "title": "Higher-order semantics for quantum programming languages with classical control",
    "authors": [
        {
            "name": "GP Atzemoglou",
            "citations_all": null,
            "citations_recent": null,
            "h_index_all": null,
            "h_index_recent": null,
