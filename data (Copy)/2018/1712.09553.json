{
    "arxiv_id": "1712.09553",
    "title": "DeepIEP: a Peptide Sequence Model of Isoelectric Point (IEP/pI) using Recurrent Neural Networks (RNNs)",
    "authors": [
        {
            "name": "EJ Bjerrum",
            "citations_all": 4903,
            "citations_recent": 4607,
