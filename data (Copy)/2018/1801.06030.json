{
    "arxiv_id": "1801.06030",
    "title": "Multi-measures fusion based on multi-objective genetic programming for full-reference image quality assessment",
    "authors": [
        {
            "name": "N Merzougui",
            "citations_all": null,
            "citations_recent": null,
