    "arxiv_id": "2112.13560",
    "title": "A Survey of Event-triggered Control for Nonlinear Multiagent Systems with Guaranteed Steady-State Performance",
    "authors": [
        {
            "name": "GM Debele",
            "citations_all": 73,
            "citations_recent": 72,
            "h_index_all": 2,
