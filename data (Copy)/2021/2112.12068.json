{
    "arxiv_id": "2112.12068",
    "title": "Metaverse Shape of Your Life for Future: A bibliometric snapshot",
    "authors": [
        {
            "name": "M Damar",
            "citations_all": 3421,
            "citations_recent": 2441,
            "h_index_all": 20,
            "h_index_recent": 18,
