{
    "arxiv_id": "2112.13072",
    "title": "A Triangular Fuzzy based Multicriteria Decision Making Approach for Assessing Security Risks in 5G Networks",
    "authors": [
        {
            "name": "HA Kholidy",
            "citations_all": 2520,
            "citations_recent": 2102,
            "h_index_all": 29,
