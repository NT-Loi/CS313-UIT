{
    "arxiv_id": "1412.8104",
    "title": "Performance Comparison of Minimum Hop vs. Minimum Edge Based Multicast Routing under Different Mobility Models for Mobile Ad hoc Networks",
    "authors": [
        {
            "name": "N Meghanathan",
            "citations_all": 3751,
            "citations_recent": 1261,
            "h_index_all": 31,
