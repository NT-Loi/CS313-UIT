{
    "arxiv_id": "1412.6781",
    "title": "Polarities & Focussing: a journey from Realisability to Automated Reasoning",
    "authors": [
        {
            "name": "S Graham-Lengrand",
            "citations_all": 1152,
            "citations_recent": 387,
            "h_index_all": 19,
            "h_index_recent": 12,
            "i10_index_all": 34,
            "i10_index_recent": 15
