{
    "arxiv_id": "2012.13016",
    "title": "Antitrust and Artificial Intelligence (AAI): Antitrust Vigilance Lifecycle and AI Legal Reasoning Autonomy",
    "authors": [
        {
            "name": "L Eliot",
            "citations_all": 518,
            "citations_recent": 261,
            "h_index_all": 11,
