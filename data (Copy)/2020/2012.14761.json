{
    "arxiv_id": "2012.14761",
    "title": "Data-driven audio recognition: a supervised dictionary approach",
    "authors": [
        {
            "name": "I Rida",
            "citations_all": 2060,
            "citations_recent": 1775,
            "h_index_all": 28,
