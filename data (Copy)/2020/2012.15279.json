{
    "arxiv_id": "2012.15279",
    "title": "Some Algorithms on Exact, Approximate and Error-Tolerant Graph Matching",
    "authors": [
        {
            "name": "SP Dwivedi",
            "citations_all": 163,
            "citations_recent": 121,
            "h_index_all": 8,
            "h_index_recent": 7,
            "i10_index_all": 8,
            "i10_index_recent": 5
