{
    "arxiv_id": "2012.15440",
    "title": "Adaptive filters for the moving target indicator system",
    "authors": [
        {
            "name": "BN Oreshkin",
            "citations_all": 7256,
            "citations_recent": 6430,
            "h_index_all": 22,
