{
    "arxiv_id": "1212.6106",
    "title": "A tropical extremal problem with nonlinear objective function and linear inequality constraints",
    "authors": [
        {
            "name": "N Krivulin",
            "citations_all": 1548,
            "citations_recent": 503,
