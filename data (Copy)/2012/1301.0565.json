{
    "arxiv_id": "1301.0565",
    "title": "An Information-Theoretic External Cluster-Validity Measure",
    "authors": [
        {
            "name": "BE Dom",
            "citations_all": null,
            "citations_recent": null,
            "h_index_all": null,
            "h_index_recent": null,
            "i10_index_all": null,
            "i10_index_recent": null
