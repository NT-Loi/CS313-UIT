{
    "arxiv_id": "1211.5122",
    "title": "The 1-2-3 Conjecture and related problems: a survey",
    "authors": [
        {
            "name": "B Seamone",
            "citations_all": 423,
            "citations_recent": 204,
            "h_index_all": 10,
