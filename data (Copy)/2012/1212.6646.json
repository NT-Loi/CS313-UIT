{
    "arxiv_id": "1212.6646",
    "title": "Blind Adaptive MIMO Receivers for Space-Time Block-Coded DS-CDMA Systems in Multipath Channels Using the Constant Modulus Criterion",
    "authors": [
        {
            "name": "RC De Lamare",
            "citations_all": 19212,
