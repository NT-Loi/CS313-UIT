{
    "arxiv_id": "1211.5643",
    "title": "Shadows and headless shadows: a worlds-based, autobiographical approach to reasoning",
    "authors": [
        {
            "name": "L Boloni",
            "citations_all": 8520,
            "citations_recent": 2792,
            "h_index_all": 40,
