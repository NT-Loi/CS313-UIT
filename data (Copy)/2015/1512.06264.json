{
    "arxiv_id": "1512.06264",
    "title": "Flexible Widely-Linear Multi-Branch Decision Feedback Detection Algorithms for Massive MIMO Systems",
    "authors": [
        {
            "name": "RC de Lamare",
            "citations_all": 19212,
            "citations_recent": 8250,
            "h_index_all": 78,
