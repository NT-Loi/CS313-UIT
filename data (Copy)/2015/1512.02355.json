{
    "arxiv_id": "1512.02355",
    "title": "Is Hamming distance the only way for matching binary image feature descriptors?",
    "authors": [
        {
            "name": "E Bostanci",
            "citations_all": 2023,
            "citations_recent": 1521,
            "h_index_all": 24,
