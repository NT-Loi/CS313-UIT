{
    "arxiv_id": "1512.04052",
    "title": "Big Data Scaling through Metric Mapping: Exploiting the Remarkable Simplicity of Very High Dimensional Spaces using Correspondence Analysis",
    "authors": [
        {
            "name": "F Murtagh",
            "citations_all": 30100,
