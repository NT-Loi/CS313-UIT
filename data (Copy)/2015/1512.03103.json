{
    "arxiv_id": "1512.03103",
    "title": "Polynomial Heisenberg algebras and Painlevé equations",
    "authors": [
        {
            "name": "D Bermudez",
            "citations_all": 848,
            "citations_recent": 519,
            "h_index_all": 15,
            "h_index_recent": 11,
            "i10_index_all": 22,
