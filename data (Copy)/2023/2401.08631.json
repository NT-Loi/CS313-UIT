{
    "arxiv_id": "2401.08631",
    "title": "The Conquest of Quantum Genetic Algorithms: The Adventure to Cross the Valley of Death",
    "authors": [
        {
            "name": "R Lahoz-Beltra",
            "citations_all": 1530,
            "citations_recent": 526,
            "h_index_all": 20,
