{
    "arxiv_id": "2401.05392",
    "title": "AT-2FF: Adaptive Type-2 Fuzzy Filter for De-noising Images Corrupted with Salt-and-Pepper",
    "authors": [
        {
            "name": "V Singh",
            "citations_all": 386,
            "citations_recent": 243,
            "h_index_all": 10,
